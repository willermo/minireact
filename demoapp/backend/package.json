{
  "name": "ft_transcendence-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "knex": "node --import tsx ./node_modules/.bin/knex",
    "migrate:latest": "npm run knex -- --knexfile src/db/knexfile.ts migrate:latest",
    "migrate:latest:prod": "node ./node_modules/.bin/knex --knexfile dist/db/knexfile.js migrate:latest",
    "migrate:rollback": "npm run knex -- --knexfile src/db/knexfile.ts migrate:rollback",
    "migrate:make": "npm run knex -- --knexfile src/db/knexfile.ts migrate:make",
    "migrate:status": "npm run knex -- --knexfile src/db/knexfile.ts migrate:status",
    "lint": "echo 'No linter configured'",
    "test": "echo 'No tests configured'"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "fastify": "^5.3.3",
    "fastify-type-provider-zod": "^5.0.1",
    "file-type": "^21.0.0",
    "google-auth-library": "^9.15.1",
    "knex": "^3.1.0",
    "nodemailer": "^7.0.3",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "sqlite3": "^5.1.6",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/node-fetch": "^2.6.12",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io": "^3.0.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
